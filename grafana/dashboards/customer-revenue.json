    {
      "id": 7,
      "type": "barchart",
      "title": "Top 20 Customers by Revenue",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "datasource": {
        "type": "postgres",
        "uid": "TimescaleDB"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT customer_id as metric, MAX(total_revenue) as value FROM customer_revenue WHERE time >= $__timeFrom()::timestamptz AND time <= $__timeTo()::timestamptz GROUP BY customer_id ORDER BY value DESC LIMIT 20;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 8,
      "type": "table",
      "title": "Customer Revenue Details",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "datasource": {
        "type": "postgres",
        "uid": "TimescaleDB"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT customer_id, MAX(total_revenue) as max_revenue, MIN(total_revenue) as min_revenue, COUNT(*) as record_count, MIN(time) as first_order, MAX(time) as last_order FROM customer_revenue WHERE time >= $__timeFrom()::timestamptz AND time <= $__timeTo()::timestamptz GROUP BY customer_id ORDER BY max_revenue DESC LIMIT 50;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "max_revenue"
            },
            "properties": [
              {
                "id": "unit",
                "value": "currencyUSD"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "min_revenue"
            },
            "properties": [
              {
                "id": "unit",
                "value": "currencyUSD"
              }
            ]
          }
        ]
      }
    }
