            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT order_date as time, COUNT(*) as order_count FROM orders WHERE order_date >= $__timeFrom()::timestamptz AND order_date <= $__timeTo()::timestamptz GROUP BY order_date ORDER BY time;",
          "refId": "A"
        }
