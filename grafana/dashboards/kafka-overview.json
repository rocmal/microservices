{
  "id": null,
  "title": "Kafka Overview",
  "schemaVersion": 16,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "Messages In Per Second",
      "targets": [
        {
          "expr": "rate(kafka_server_brokertopicmetrics_messages_in_total[1m])",
          "legendFormat": "{{topic}}",
          "refId": "A"
        }
      ],
      "datasource": "Prometheus",
      "id": 1
    },
    {
      "type": "graph",
      "title": "Request Handler Avg Idle Percent",
      "targets": [
        {
          "expr": "avg(kafka_server_kafkarequesthandlerpool_requesthandleravgidle_percent)",
          "legendFormat": "idle",
          "refId": "B"
        }
      ],
      "datasource": "Prometheus",
      "id": 2
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}
