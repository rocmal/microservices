          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT osh.order_line_id AS \"Order ID\", otm.name AS \"Order Type\", osh.status AS \"Workflow Stage\", CASE WHEN osh.duration_seconds >= (osh.sla_target_minutes * 60) THEN 'Delayed' WHEN osh.duration_seconds > (osh.sla_target_minutes * 60 * 0.25) THEN 'Moving Slow' ELSE 'SLA Met' END AS \"SLA Status\", (osh.sla_target_minutes - ROUND(osh.duration_seconds / 60.0, 0)) AS \"Time Remaining\", osh.duration_seconds AS \"Duration (sec)\", osh.entered_date AS \"Entered Time\" FROM order_status_history osh JOIN order_type_master otm ON osh.order_type_id = otm.id WHERE $__timeFilter(osh.entered_date) AND osh.status = '${workflow_status}' AND (CASE WHEN osh.duration_seconds >= (osh.sla_target_minutes * 60) THEN 'Delayed' WHEN osh.duration_seconds > (osh.sla_target_minutes * 60 * 0.25) THEN 'Moving Slow' ELSE 'SLA Met' END) = '${sla_status}' AND ('$__all' IN (${order_type:singlequote}) OR otm.name IN (${order_type:singlequote})) ORDER BY osh.entered_date DESC LIMIT 500",
          "refId": "A"
        }
      ],
      "title": "Order-Level SLA Details",
      "type": "table"
    }
